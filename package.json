{
  "name": "@shellicar/pino-applicationinsights-transport",
  "version": "1.0.1",
  "private": false,
  "description": "Azure Application Insights transport for pino",
  "packageManager": "pnpm@9.11.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup-node --watch",
    "build": "tsup-node",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "mocha",
    "prepublishOnly": "run-p lint build",
    "postpublish": "pnpm version --no-git-tag-version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellicar/pino-applicationinsights-transport.git"
  },
  "keywords": [
    "pino",
    "azure",
    "insights",
    "applicationinsights",
    "transport",
    "logging"
  ],
  "author": "Stephen Hellicar",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.11.0",
    "eslint-plugin-import": "^2.29.0",
    "globals": "^15.9.0",
    "logform": "^2.6.1",
    "mocha": "^10.7.3",
    "npm-run-all": "^4.1.5",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "terser": "^5.33.0",
    "testdouble": "^3.20.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "applicationinsightsv2": "npm:applicationinsights@^2",
    "applicationinsightsv3": "npm:applicationinsights@^3"
  },
  "peerDependencies": {
    "pino": "^9.0.0",
    "pino-abstract-transport": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/shellicar/pino-applicationinsights-transport/issues"
  },
  "homepage": "https://github.com/shellicar/pino-applicationinsights-transport#readme"
}
